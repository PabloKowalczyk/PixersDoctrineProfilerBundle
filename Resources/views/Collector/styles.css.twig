.sf-reset .traces {
    padding: 0 0 1em 1.5em;
}

.sf-reset .traces abbr {
    border-bottom-color: #aaa;
    padding-bottom: 2px;
}
.sf-reset .traces a {
    color: #868686;
    text-decoration: none;
}

.sf-reset .traces li {
    font-size: 12px;
    color: #868686;
    padding: 5px 4px;
    list-style-type: decimal;
    margin:0 0 0 20px;
    line-height: normal;
    
}
.sf-reset ol {
    margin: 0;
    padding: 10px 0;
}
.sf-reset strong {
    color: #313131;
    font-weight: bold;
}
.sf-reset .traces li.selected {
    background: rgba(255, 255, 153, 0.5);
}

.sf-reset .traces ol li {
    font-size: 12px;
    color: #777;
    list-style-position: inside;
}

.sf-reset .trace {
    border: 1px solid #DDD;
    background: #FFF;
    padding: 10px;
    overflow: auto;
    margin: 1em 0;
    white-space: nowrap;
}
.sf-reset .trace code {
    font-size: 13px;
    white-space: nowrap;
}

.sf-reset .toggle {
    vertical-align: middle;
}

.container {
    max-width: none;
}

.toggleicon,
.sf-reset .toggleicon, 
.sf-reset .toggleicon:hover {
    width: 16px;
    height: 16px;
    display: inline-block;
    background: url("data:image/png;base64,") no-repeat bottom left #5eb5e0;
}
.toggleicon--open, 
.sf-reset .toggleicon--open {
    background-position: top left;
}

.popup__shadow {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.6);
    z-index: 10000;
}

.popup__close {
    position: absolute;
    right: 0px;
    top: 0px;
    transform: translate(50%, -50%);
    -webkit-transform: translate(50%, -50%);
    background: #aaa;
    color: #fff;
    width: 20px;
    height: 20px;
    line-height: 20px;
    text-align: center;
}

.popup__content {
    margin: 2vh;
    max-height: 86vh;
    overflow: auto;
    backface-visibility: hidden;
    transform: translateZ(0) scale(1.0, 1.0);
}

.popup__window {
    display: none;
    position: fixed;
    z-index: 10000;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    background: #fff;
    border: 4px solid #aaa;
    max-width: 90%;
    max-height: 90%;
}

.popup__content pre {
    overflow: visible;
}

.highlight-row td {
    background: rgba(240, 181, 24, 0.3);
}

.popup__content .traces {
    min-width: 50vw;
}

.tree__header,
.tree__row {
    width: 100%;
    display: table;
    border-top: 1px solid #E0E0E0;
}

.tree__header {
    background: #E0E0E0;
    font-weight: bold;
}

.tree__col {
    padding: 8px 10px;
    display: table-cell;
    width: 70px;
}

.tree__col:first-child {
    width: auto;
}

.tree-wrapper {
    background: #fff;
    border: 1px solid #E0E0E0;
    box-shadow: 0px 0px 1px rgba(128, 128, 128, .2);
}

.tree,
.tree ul {
    margin: 0 0 0 0;
    padding: 0;
    list-style: none;
    position: relative;
}

.tree__toggle {
    color: inherit;
    display: block;
}

.tree__switch {
    float: left;
    width: 20px;
    height: 20px;
    line-height: 20px;
    text-align: center;
    border: 1px solid #E0E0E0;
    position: absolute;
    left: 14px;
    top: 0;
}

.tree__switch:before {
    content: '+';
}

.tree ul {
    margin-left: -15px;
    display: none
}

.tree:before,
.tree ul:before {
    content: "";
    display: block;
    width: 0;
    position: absolute;
    top: -40px;
    bottom: 0;
    left: 0;
    border-left: 1px solid #E0E0E0;
}

.percval {
    font-size: 80%;
}

.tree li {
    margin: 10px 0;
    padding: 10px 0 0 40px;
    position: relative;
}

.tree .tree__leaf:before {
    width: 40px;
}

.tree li:before {
    content: "";
    display: block;
    width: 10px;
    height: 0;
    border-top: 1px solid #E0E0E0;
    margin-top: -1px;
    position: absolute;
    top: 11px;
    left: 0;
}

.tree li:last-child:before {
    background: white;
    height: auto;
    top: 11px;
    bottom: 0;
}

.tree--open > ul{
    display: block;
}

.tree--open > .tree__row .tree__switch:before{
    content: '-';
}